#########printing 'L' letter########
for row in range(4):
    for col in range(3):
        if (col==0 or (row==3 and row>0)):
            output = " "
            output += "x"
            print(output,end="")
        else:
            print(end=" ")
    print()
##############printing 'C' letter##########
for row in range(4):
    for col in range(3):
        if ((col==0 or row==0) or (row==3 and (col>0 or row>0))):
            output=" "
            output+= "x"
            print(output,end="")
        else:
            print(end=" ")
    print()
 #############Two dimentional list############3
 matrix = [[1,2,3],
          [4,5,6],
          [7,8,9]]
print(matrix[2][1])
 ans: it will print 5
 ###nested for loop####
for row in matrix:
    print(row)
    for item in row:
        print(item) 
output: row:[1, 2, 3]
item:1
