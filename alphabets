#########3remove duplicates###########
lis1=[2,4,5,6,2,4,7]
lis2=[]
'''for item in lis1:
    if (lis1.count(item)>1):
        lis1.remove(item)
        print(lis1)'''
for number in lis1:
    if number not in lis2:
        lis2.append(number)
print(lis2)

