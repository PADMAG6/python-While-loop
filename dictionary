#################3dictionry#########################
phone = input("Phone: ")
digits_mapping = {
    "1" : "One",
    "2" : "Two",
    "3" : "Three",
    "4" : "Four"
}
output = ""
for ch in phone:
    print(ch)
    output += digits_mapping.get(ch,"!") + " "
print(output)
##################emoji prints####################33
emoji = input(">")
words= emoji.split(' ')
#print(words)
mesage= {
    ":)" : "ğŸ˜Š",                         #for emiji in windows pycharm : windows+>.
    ":(" : "ğŸ˜‚"
    }
output = ""
for word in words:
    output+= mesage.get(word, word)
print(output)
